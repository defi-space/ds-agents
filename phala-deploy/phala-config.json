{
  "phala": {
    "deploymentId": "defi-space-agents",
    "version": "1.0.0",
    "description": "defi-space-agents running in Phala TEE - Always running 4 agents",
    "resources": {
      "cpu": "2",
      "memory": "2G",
      "storage": "1G"
    },
    "confidentiality": {
      "level": "high",
      "allowedApiEndpoints": [
        "api.starknet.io",
        "gateway.api.gemini.com"
      ]
    },
    "environment": {
      "variables": [
        "STARKNET_RPC_URL",
        "GOOGLE_API_KEY",
        "AGENT1_PRIVATE_KEY",
        "AGENT1_ADDRESS",
        "AGENT1_API_KEY",
        "AGENT2_PRIVATE_KEY",
        "AGENT2_ADDRESS",
        "AGENT2_API_KEY",
        "AGENT3_PRIVATE_KEY",
        "AGENT3_ADDRESS",
        "AGENT3_API_KEY",
        "AGENT4_PRIVATE_KEY",
        "AGENT4_ADDRESS",
        "AGENT4_API_KEY",
        "INDEXER_URL"
      ],
      "fixed": {
        "AGENT_COUNT": "4",
        "PHALA_TEE": "true",
        "NODE_ENV": "production",
        "ENABLE_DASHBOARD": "false"
      }
    },
    "networking": {
      "inbound": false,
      "outbound": true
    },
    "attestation": {
      "required": true,
      "minLevel": "secure"
    },
    "registry": {
      "image": "ghcr.io/wayzeek/defi-space-agents:latest",
      "type": "github"
    }
  }
} 